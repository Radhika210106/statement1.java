code 1:
class Cars {
    void sound(){
        System.out.println("cars make a sound");
    }
}
class BMW extends Cars{
     void sound(){
        System.out.println("BMW");
     }
}
class Audi extends Cars{
     void sound(){
        System.out.println("Audi");
     }
}
class Benz extends Cars{
     void sound(){
        System.out.println("Benz");
     }
}
class Main {
    public static void main(String[] args) {
        Cars a;
        a=new BMW();
        a.sound();
        a=new Audi();
        a.sound();
        a=new Benz();
        a.sound();
         }  
}
output:
BMW
Audi
Benz

code2:
class grandparents {
    void sound(){
        System.out.println("grandparent");
    }
}
class grandfather extends grandparents{
     void sound(){
        System.out.println("narayanan");
     }
}
class grandmother  extends grandparents{
     void sound(){
        System.out.println("kalaiselvi");
     }
}
class father extends grandparents{
     void sound(){
        System.out.println("saravanan");
     }
}
class mother extends grandparents{
     void sound(){
        System.out.println("chandra");
     }
}


class Main {
    public static void main(String[] args) {
        grandparents a;
        a=new grandfather();
        a.sound();
        a=new grandmother();
        a.sound();
        a=new father();
        a.sound();
        a=new mother();
        a.sound();
        
        
    }  
}
output:
narayanan
kalaiselvi
saravanan
chandra
code 3:
import java.util.Scanner;

class grandparents {
    String name;

    grandparents(String name) {
        this.name = name;
    }
    void sound() {
        System.out.println(name);
    }
}
class grandfather extends grandparents {
    grandfather(String name) {
        super(name);
    }
}
class grandmother extends grandparents {
    grandmother(String name) {
        super(name);
    }
}
class father extends grandparents {
    father(String name) {
        super(name);
    }
}
class mother extends grandparents {
    mother(String name) {
        super(name);
    }
}
public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        System.out.print("Enter grandfather name: ");
        grandparents f1 = new grandfather(sc.nextLine());
        System.out.print("Enter grandmother name: ");
         grandparents f2 = new grandmother(sc.nextLine());
        System.out.print("Enter father name: ");
        grandparents f3 = new father(sc.nextLine());
        System.out.print("Enter mother name: ");
         grandparents f4 = new mother(sc.nextLine());
        f1.sound();
        f2.sound();
        f3.sound();
        f4.sound();
        sc.close();
    }
}
output:
Enter grandfather name: nayarayanan
Enter grandmother name: kalaiselvi
Enter father name: saravanan
Enter mother name: chandra
nayarayanan
kalaiselvi
saravanan
chandra
code 4:
class Father {
    void sound() {
        System.out.println("Saravanan");
    }
}

class Mother {
    void sound() {
        System.out.println("Saravanan's wife");
    }
}

class Son extends Father {
    void sound() {
        System.out.println("magesh");
    }
}

public class Main {
    public static void main(String[] args) {
        Father a = new Son();
        a.sound();  
    }
}
output:
magesh
code 5:
class Vehicle {
    Vehicle() {
        System.out.println("this is wheeler");
    }
}

class Fourwheeler extends Vehicle {
    Fourwheeler() {
        System.out.println("this is fourwheeler");
    }
}

class Car extends Fourwheeler {
   Car() {
        System.out.println("this is car");
    }
}


public class Main {
    public static void main(String[] args) {
        Car obj=new Car();
    }
}
output:
this is wheeler
this is fourwheeler
this is car
code 6:
import java.util.Scanner;

class grandparents{
    grandparents() {
        System.out.println("thaatha");
    }
}

class cousins extends grandparents{
    cousins() {
        System.out.println("cousin");
    }
}

class  daughter  extends cousins{
    daughter() {
        System.out.println("myself radhi");
    }
}

public class Main {
    public static void main(String[] args) {
        
        daughter obj = new daughter();

      
    }
}
output:
thaatha
cousin
myself radhi
code 7:
class Grandparents {
    Grandparents() {
        System.out.println("thaatha");
    }
}

class Cousins extends Grandparents {
    Cousins() {
        System.out.println("cousin");
    }
}

class Daughter extends Grandparents {
    Daughter() {
        System.out.println("my self radhuu");
    }
}

public class Main {
    public static void main(String[] args) {
        Cousins cousinObj = new Cousins();
        Daughter daughterObj = new Daughter();
    }
}
output:
thaatha
cousin
thaatha
my self radhuu



