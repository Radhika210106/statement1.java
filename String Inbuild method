class Main {
    public static void main(String[] args) {
        
        char[] data = {'9', '3', '0', '1', '4','5','6','7','8'};
    
    
        System.out.println(data);
        
        String mystring ="radhi";
        mystring.getChars(0,5,data,4);
        
        System.out.println(data);
    
    }
}

