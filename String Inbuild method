import java.util.*;

class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        String data = "radhi";
        String Hi="Hi %s my age  is %d";
        System.out.println(data.equalsIgnoreCase(Hi));
        System.out.printf(Hi,"radhi",21);
        
    }
}
output:
false
Hi radhi my age  is 21
