import java.util.Scanner;

class Main {
    public static void main(String[] args) {
       
        Scanner x = new Scanner(System.in);

        System.out.println("Enter a string:");
        String data = x.nextLine();

        char one = data.charAt(0);      
        int two = data.codePointAt(3);           
        int three = data.codePointBefore(4);     

        System.out.println("one: " + one);
        System.out.println("two: " + two);
        System.out.println("three: " + three);

        x.close();
    }
}
output:
Enter a string:
Radhika
one: R
two: 104
three: 104
code 2:
import java.util.Scanner;

class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);

        System.out.println("Enter first string:");
        String first = sc.nextLine();

        System.out.println("Enter second string:");
        String second = sc.nextLine();

        
        int codePointCount = first.codePointCount(0, first.length());

        int comparisonResult = first.compareTo(second);

        System.out.println("Code point  " + codePointCount);
        System.out.println("Comparison : " + comparisonResult);

        sc.close();
    }
}
output:
Enter first string:
radhi
Enter second string:
raddhi
Code point : 5
Comparison : 4
code 3:
import java.util.Scanner;

class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);

        System.out.println("Enter first string:");
        String first = sc.nextLine();

        System.out.println("Enter second string:");
        String second = sc.nextLine();

        
        int comparecase = first.compareToIgnoreCase(second);

        String concatcase = second.concat(first);

        System.out.println("Code point : " + comparecase);
        System.out.println("Comparison : " + concatcase);

        sc.close();
    }
}
output:
Enter first string:
radhika
Enter second string:
jevigaa
Code point : 8
Comparison : jevigaaradhika
