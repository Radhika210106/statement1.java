import java.util.Scanner;

class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);

        System.out.println("Enter first string:");
        String first = sc.nextLine();

        System.out.println("Enter second string:");
        String second = sc.nextLine();

        
        int codePointCount = first.codePointCount(0, first.length());

        int comparisonResult = first.compareTo(second);

        System.out.println("Code point  " + codePointCount);
        System.out.println("Comparison : " + comparisonResult);

        sc.close();
    }
}
output:
Enter first string:
radhi
Enter second string:
raddhi
Code point : 5
Comparison : 4
code 3:
import java.util.Scanner;

class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);

        System.out.println("Enter first string:");
        String first = sc.nextLine();

        System.out.println("Enter second string:");
        String second = sc.nextLine();

        
        int comparecase = first.compareToIgnoreCase(second);

        String concatcase = second.concat(first);

        System.out.println("Code point : " + comparecase);
        System.out.println("Comparison : " + concatcase);

        sc.close();
    }
}
output:
Enter first string:
radhika
Enter second string:
jevigaa
Code point : 8
Comparison : jevigaaradhika
code 4:
import java.util.Scanner;

class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);

        System.out.println("Enter first string:");
        String first = sc.nextLine();

        System.out.println("Enter second string:");
        String second = sc.nextLine();

        
        boolean con = first.contains(second);

        boolean coneq = second.contentEquals(first);

        System.out.println("Code point : " + con);
        System.out.println("Comparison : " + coneq);

        sc.close();
    }
}
output:
Enter first string:
radhi
Enter second string:
rad
Code point : true
Comparison : false
