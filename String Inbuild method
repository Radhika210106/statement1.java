import java.util.*;
class Main {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        char[] data = {'r', 'a', 'd', 'h', 'i'};
        String str =" ";
        str = str.copyValueOf(data, 0, 5);
        System.out.println(str.endsWith("h"));
        System.out.println(str);
        scanner.close();
    }
}
output:
false
radhi
