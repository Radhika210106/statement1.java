class Main {
    public static void main(String[] args) {
        
        int [][] arr;
        arr=new int[1][3];
        arr[0][0]=2;
         arr[0][1]=5;
          arr[0][2]=7;
        
        System.out.println(arr[0][1]);
    }
}
output:
5
 
